                alert("请选择我的评分");
                return;
            }
            else {
                if (Fraction <= 2 && $.trim($("#txtContent").val()) == "") {
                    alert("请填写评价内容");
                    return;
                }

            }
            var josnStr = "{\"vFraction\":\"" + Fraction + "\" ,\"vCn\":\"" + $("#txtContent").val() + "\"}";
